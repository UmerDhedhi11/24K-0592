Q2: 
INPUT Num1 = 12
INPUT Num2 = 24
INPUT Num3 = 7
IF 
   Num1 > Num2 AND Num2 > Num3
   THEN PRINT "Num1 is the Maximum"
ELSEIF
   Num2 > Num1 AND Num1 > Num3
   Then PRINT "Num2 is the Maximum"
ELSE
   PRINT "Num3 is the Maximum"
ENDIF

Q3:
INPUT Num1 = 23
INPUT Num2 = -12
INPUT Num3 = -5
Sum = Num1 - Num2 - Num3
PRINT Sum

Q4:
OUTPUT "Enter two numbers: " 
INPUT Num1, Num2
OUTPUT "Select + or - for addition or subtraction: "
INPUT Operator
IF 
   Operator == '+'
   THEN Answer = Num1 + Num2
   PRINT "Answer is: ", Answer
ELSEIF 
   Operator == '-'
   THEN Answer = Num1 - Num2
   PRINT "Answer is: ", Answer
ELSE
   PRINT "Invalid Input"
ENDIF

Q5:
OUTPUT "Enter number to be divided: "
INPUT Num
OUTPUT "Enter number to be divided from: "
INPUT Div
IF 
   Num MOD Div == 0
   IF Num MOD 2 == 0 
      PRINT "It is a divisor and it is even"
   ELSE
      PRINT "It is a divisor and it is odd
ELSE
   PRINT "It is not a divisor"
   ENDIF
ENDIF

Q6:
OUTPUT "Enter a number between 1-12: "
INPUT Num
IF
   Num == 1
   PRINT "January"
ELSEIF
   Num == 2
   PRINT "February"
ELSEIF
   Num == 3
   PRINT "March"
ELSEIF
   Num == 4
   PRINT "April"
ELSEIF
   Num == 5
   PRINT "May"
ELSEIF
   Num == 6
   PRINT "June"
ELSEIF
   Num == 7
   PRINT "July"
ELSEIF
   Num == 8
   PRINT "August"
ELSEIF
   Num == 9
   PRINT "September"
ELSEIF
   Num == 10
   PRINT "October"
ELSEIF
   Num == 11
   PRINT "November"
ELSEIF
   Num == 12
   PRINT "December"
ELSE
   PRINT "Invalid Number"
ENDIF

Q7:
OUTPUT "Enter two numbers: "
INPUT Num1, Num2
OUTPUT "Enter +,-,*,/,% to perform operation: "
Input Op
IF
   Op == '+'
   THEN PRINT (Num1 + Num2)
ELSEIF
   Op == '-'
   THEN PRINT (Num1 - Num2)
ELSEIF
   Op == '*'
   THEN PRINT (Num1 * Num2)
ELSEIF
   Op == '/'
   THEN PRINT (Num1/Num2)
ELSEIF
   Op == '%'
   THEN PRINT (Num1%Num2)
ELSE
   PRINT "Invalid Input
