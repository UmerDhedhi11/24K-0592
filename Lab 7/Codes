Q1:
#include <stdio.h>

int main(){
int i, arr[6]={21,45,54,12,2,100}, lastind;
lastind = arr[5];
for(i=5;i>=1;i--){
arr[i]=arr[i-1];
}
arr[0]=lastind;
printf("Array after shift: ");
for(i=0;i<=5;i++){
printf("%d | ", arr[i]);
}
}

Q2:
#include <stdio.h>

int main(){
char arr[11] = "Hello World";
int cta = 0, cte = 0, cti = 0, cto = 0, ctu = 0, i;
for(i=0;i<=10;i++){
switch(arr[i]){
case 'a':
cta = cta + 1;
break;
case 'e':
cte = cte + 1;
break;
case 'i':
cti = cti + 1;
break;
case 'o':
cto = cto + 1;
break;
case 'u':
ctu = ctu + 1;
break;
}
}
printf("a=%d, e=%d, i=%d, o=%d, u=%d", cta,cte,cti,cto,ctu);
}

Q3:
#include <stdio.h>

int main(){
    char string[] = "Hello Everybody";
    char *startp = string;
    char *endp = string;
    int l = 0;
    char temp;
    
    while(*endp != '\0'){
        endp++;
    }
    endp = endp - 1;
    while(startp <= endp){
        l++;
        startp++;
    }
    printf("The length of string is %d\n", l);
    startp = string;
    while (startp < endp){
        temp = *startp;
        *startp = *endp;
        *endp = temp;
        startp += 1;
        endp -= 1;
    }
    printf("The reversed string is: %s", string);
}

Q4:
#include <stdio.h>

int main() {
    char str[20];
    int x = 0,i = 0;
    int pal;
    printf("Enter a word: ");
    scanf("%s", str);
    
    while(str[x] != '\0'){
        x++;
    }
    x--;
    while(i < x){
        if(str[i] == str[x]){
            pal = 1;
            i++;
            x--;
        }
        else{
            pal = 0;
            break;
        }
    }
    if(pal == 1){
        printf("%s is a palindrome", str);
    }
    else{
        printf("%s is not a palindrome", str);
    }
}

