Q1:
#include <stdio.h>

int main(){
int i, arr[6]={21,45,54,12,2,100}, lastind;
lastind = arr[5];
for(i=5;i>=1;i--){
arr[i]=arr[i-1];
}
arr[0]=lastind;
printf("Array after shift: ");
for(i=0;i<=5;i++){
printf("%d | ", arr[i]);
}
}

Q2:
#include <stdio.h>

int main(){
char arr[50];
int cta = 0, cte = 0, cti = 0, cto = 0, ctu = 0, i = 0;
printf("Enter a string without spaces. You have space upto 49 characters: ");
scanf("%s", arr);

while(arr[i] != '\0'){
    switch(arr[i]){
        case 'a':
        cta = cta + 1;
        break;
        case 'e':
        cte = cte + 1;
        break;
        case 'i':
        cti = cti + 1;
        break;
        case 'o':
        cto = cto + 1;
        break;
        case 'u':
        ctu = ctu + 1;
        break;
}
i++;
}
printf("a=%d, e=%d, i=%d, o=%d, u=%d", cta,cte,cti,cto,ctu);
}

Q3:
#include <stdio.h>

int main(){
    char string[] = "Hello Everybody";
    char *startp = string;
    char *endp = string;
    int l = 0;
    char temp;
    
    while(*endp != '\0'){
        endp++;
    }
    endp = endp - 1;
    while(startp <= endp){
        l++;
        startp++;
    }
    printf("The length of string is %d\n", l);
    startp = string;
    while (startp < endp){
        temp = *startp;
        *startp = *endp;
        *endp = temp;
        startp += 1;
        endp -= 1;
    }
    printf("The reversed string is: %s", string);
}

Q4:
#include <stdio.h>

int main() {
    char str[20];
    int x = 0,i = 0;
    int pal;
    printf("Enter a word: ");
    scanf("%s", str);
    
    while(str[x] != '\0'){
        x++;
    }
    x--;
    while(i < x){
        if(str[i] == str[x]){
            pal = 1;
            i++;
            x--;
        }
        else{
            pal = 0;
            break;
        }
    }
    if(pal == 1){
        printf("%s is a palindrome", str);
    }
    else{
        printf("%s is not a palindrome", str);
    }
}

Q5:
#include <stdio.h>

int main(){
    int arr[10];
    int freq[100] = {0};
    int i;
    printf("Enter a number between 0 and 99 inclusive: \n");
    for(i=0;i<=9;i++){
        scanf("%d", &arr[i]);
        freq[arr[i]]++;
    }
    for(i=0;i<=99;i++){
        if(freq[i]>0){
            printf("\n%d: %d times\n", i, freq[i]);
        }
    }
}

Q6:
#include <stdio.h>

int main(){
    int num[50];
    int freq[50] = {0};
    int n, i = 0, temp;
    printf("Enter numbers between 1 and 49. Enter 0 when you are done: \n");
    while(1){
        scanf("%d", &temp);
        if(temp == 0){
            break;
        }
        else{
            num[i] = temp;
            i++;
            n++;
        }
    }
    for(i=0;i<=n;i++){
        freq[num[i]]++;
        if(num[i] != 0){
            if(freq[num[i]] > 1){
                printf("%d occurs more than once\n", num[i]);
                num[i] = 0;
            }
        }    
    }
}

